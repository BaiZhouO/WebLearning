<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 设置为UserDao接口方法提供sql语句配置 -->
<mapper namespace="com.BuyingSystem.dao.StaffDao">


    <select id="selectStaffById" resultType="Staff" parameterType="int">
        select * from staff where staff_id = #{staff_id}
    </select>



    <select id="selectStaffByName" parameterType="java.lang.String" resultType="Staff">
        select * from staff where loginName = #{loginName}
    </select>


</mapper>